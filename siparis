string[] siparisNo = { "Birinci Siparis", "Ikinci Siparis", "Ucuncu Siparis", "Dorduncu Siparis" };
int[] fiyat = { 250, 300, 350, 400 };
int toplamFiyat = 0;
int say = 0;

foreach (string siparis in siparisNo)
{
    Console.WriteLine(siparis); // döngü x4
}

foreach (var tekilFiyat in fiyat)
{
    toplamFiyat += tekilFiyat;
    say++;
}
